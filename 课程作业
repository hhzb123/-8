#include<stdio.h>
#include<stdlib.h>
#include<time.h>
#define N 12
#include"头文件.h"

void randomNumber()
{
	         a=rand()%N;
	     	 b=rand()%N;
	     	 c=rand()%N;
	     	 d=rand()%N;
		 	 e=rand()%N;
		 	 f=rand()%N;
		 	 g=rand()%N;
		 	 h=rand()%N;
		 	 k=rand()%N;
		 	 l=rand()%N;
}

void calculateResult(){
	gb=gongbei(a,b,c,d);
		 	 sum=gb/a*e+gb/b*f+gb/c*g+gb/d*h;
		  	 gy=gongyue(sum,gb);
		 	 sum=sum/gy;
		 	 gb=gb/gy;
		 	 if(sum<0&&gb<0) {
		 	 sum=-sum;
		 	 gb=-gb;
		                     }
		      if(sum>0&&gb<0){
		      	sum=-sum;
		      	gb=-gb;
		                      }               
		 	 
		 	
		 	 ce=(double)sum/(double)gb;
		 	 zhengque=ce;
}
